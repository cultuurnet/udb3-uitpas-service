{
    "name": "cultuurnet/udb3-uitpas-service",
    "description": "Silex application for UiTPAS integration",
    "type": "project",
    "license": "GPLv3",
    "authors": [],
    "require": {
        "silex/silex": "~1.3",
        "cultuurnet/udb3": "~0.1",
        "cultuurnet/broadway-tools": "~0.1",
        "deralex/yaml-config-service-provider": "~1.0",
        "knplabs/console-service-provider": "~1.0",
        "jdesrosiers/silex-cors-provider": "~0.1.4",
        "monolog/monolog": "~1.11",
        "doctrine/mongodb": "~1.3",
        "doctrine/migrations": "~1.0",
        "broadway/broadway": "~0.10.0",
        "cultuurnet/silex-amqp": "~1.0",
        "cultuurnet/silex-service-provider-jwt": "~1.0",
        "cultuurnet/symfony-security-jwt": "~1.0",
        "cultuurnet/udb3-http-foundation": "~0.1",
        "cultuurnet/monolog-socketio": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "escapestudios/symfony2-coding-standard": "~2.9",
        "squizlabs/php_codesniffer": "~2.5.1",
        "phing/phing": "~2.14",
        "satooshi/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\UiTPASService\\": [
                "src/",
                "app/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\UiTPASService\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "5.6.27",
            "ext-redis": "2.2.7.0",
            "ext-mongo": "1.6.14"
        }
    }
}
